<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Face ID Usage Description -->
    <key>NSFaceIDUsageDescription</key>
    <string>MedSalary usa Face ID para un inicio de sesión rápido y seguro.</string>
    
    <!-- URL Types for OAuth callbacks -->
    <key>CFBundleURLTypes</key>
    <array>
        <!-- Google Sign-In URL Scheme (placeholder - replace with actual) -->
        <dict>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>com.googleusercontent.apps.YOUR-CLIENT-ID</string>
            </array>
            <key>CFBundleURLName</key>
            <string>Google Sign-In</string>
        </dict>
        <!-- Facebook URL Scheme (placeholder - replace with actual) -->
        <dict>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>fbYOUR-APP-ID</string>
            </array>
            <key>CFBundleURLName</key>
            <string>Facebook Login</string>
        </dict>
    </array>
    
    <!-- Facebook SDK configuration (placeholder) -->
    <key>FacebookAppID</key>
    <string>YOUR-FACEBOOK-APP-ID</string>
    <key>FacebookClientToken</key>
    <string>YOUR-FACEBOOK-CLIENT-TOKEN</string>
    <key>FacebookDisplayName</key>
    <string>MedSalary</string>
    
    <!-- Queried URL Schemes for social apps -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
        <string>fbapi</string>
        <string>fb-messenger-api</string>
        <string>fbauth2</string>
        <string>fbshareextension</string>
    </array>
</dict>
</plist>
